{
  "name": "Antibias Chrome Extension",
  "version": "0.1",
  "description": "Chrome extension designed to reduce implicit bias by obfuscating profile names and images on LinkedIn.",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "48": "images/48x48.png",
      "128": "images/128x128.png"
    }
  },
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "manifest_version": 2
}
